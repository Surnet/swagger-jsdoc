{
  "name": "swagger-jsdoc",
  "version": "3.2.9",
  "description": "Generates swagger doc based on JSDoc",
  "main": "index.js",
  "scripts": {
    "test:lint": "eslint .",
    "test:js": "mocha --timeout 10000 --exit",
    "start": "node example/v2/app.js",
    "test": "run-p test:* -cn"
  },
  "bin": {
    "swagger-jsdoc": "./bin/swagger-jsdoc.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Surnet/swagger-jsdoc.git"
  },
  "keywords": [
    "jsdoc",
    "restful",
    "api",
    "express",
    "swagger"
  ],
  "author": "https://github.com/Surnet/swagger-jsdoc/graphs/contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Surnet/swagger-jsdoc/issues"
  },
  "homepage": "https://github.com/Surnet/swagger-jsdoc",
  "dependencies": {
    "commander": "2.17.1",
    "doctrine": "2.1.0",
    "glob": "7.1.3",
    "js-yaml": "3.13.1",
    "swagger-parser": "8.0.0"
  },
  "devDependencies": {
    "body-parser": "1.18.3",
    "chai": "4.2.0",
    "chai-jest-snapshot": "2.0.0",
    "eslint": "5.5.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-loader": "2.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "2.6.2",
    "express": "4.16.3",
    "husky": "3.0.0",
    "lint-staged": "9.1.0",
    "mocha": "6.1.4",
    "npm-run-all": "4.1.5",
    "prettier": "1.14.2",
    "supertest": "4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
