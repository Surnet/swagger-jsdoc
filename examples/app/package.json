{
  "name": "swagger-jsdoc-example-app",
  "description": "Example express app using swagger-jsdoc",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "lint": "eslint .",
    "test:lint": "eslint .",
    "test:js": "NODE_OPTIONS=--experimental-vm-modules jest --no-cache",
    "test": "run-p test:* -cn"
  },
  "jest": {
    "transform": {}
  },
  "dependencies": {
    "swagger-jsdoc": "^7.0.0-rc.1"
  },
  "devDependencies": {
    "body-parser": "1.19.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.1",
    "express": "4.17.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "supertest": "6.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
